{
  "name": "client",
  "version": "0.0.5",
  "license": "MIT",
  "categories": [
    "Extension Packs"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/extension.ts --external vscode --format cjs -d dist",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "vscode-css-languageservice": "^6.3.6"
  }
}
