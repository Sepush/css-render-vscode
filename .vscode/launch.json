{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/client/out/**/*.js",
        "${workspaceFolder}/packages/server/out/**/*.js"
      ],
      "preLaunchTask": "dev"
    }
  ]
}
